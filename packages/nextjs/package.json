{
  "name": "@myzkey/eslint-config-nextjs",
  "version": "0.0.0",
  "description": "Next.js ESLint configuration",
  "keywords": [
    "eslint",
    "config",
    "nextjs",
    "next",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/myzkey/eslint-config/blob/main/packages/nextjs/README.md",
  "bugs": {
    "url": "https://github.com/myzkey/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myzkey/eslint-config.git"
  },
  "license": "MIT",
  "author": "myzkey",
  "type": "module",
  "exports": {
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.cjs"
    },
    "import": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rslib build",
    "lint:code": "eslint .",
    "lint:type": "tsc --noEmit"
  },
  "dependencies": {
    "@myzkey/eslint-config-definer": "workspace:*",
    "eslint-config-next": "^15.1.6"
  },
  "devDependencies": {
    "@rslib/core": "^0.17.1",
    "@types/eslint": "^8.56.2",
    "eslint": "^9.17.0",
    "next": "^15.1.6"
  },
  "peerDependencies": {
    "@myzkey/eslint-config-react": "workspace:*",
    "eslint": "^9.17.0",
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0"
  },
  "publishConfig": {
    "provenance": true
  }
}
