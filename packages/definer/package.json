{
  "name": "@myzkeyjs/eslint-config-definer",
  "version": "0.0.2",
  "description": "ESLint config definer utilities",
  "keywords": [
    "eslint",
    "config",
    "definer",
    "utilities"
  ],
  "homepage": "https://github.com/myzkey/eslint-config/blob/main/packages/definer/README.md",
  "bugs": {
    "url": "https://github.com/myzkey/eslint-config/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myzkey/eslint-config.git"
  },
  "license": "MIT",
  "author": "myzkey",
  "type": "module",
  "exports": {
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.cjs"
    },
    "import": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rslib build",
    "lint:code": "eslint .",
    "lint:type": "tsc --noEmit"
  },
  "devDependencies": {
    "@rslib/core": "^0.17.1",
    "@types/eslint": "^8.56.2",
    "eslint": "^9.17.0"
  },
  "peerDependencies": {
    "eslint": "^9.17.0"
  }
}
